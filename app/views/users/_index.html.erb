<table class = "table table-hover">
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
  </tr>
  <tr>
    <% @users.each do |user| %>
    <td>
      <%= image_tag user.get_profile_image(100,100) %>
    </td>
    <td>
      <%= user.name %>
    </td>
    <td><%= link_to "Show", user_path(user) %></td>
  </tr>
  <% end %>
</table>